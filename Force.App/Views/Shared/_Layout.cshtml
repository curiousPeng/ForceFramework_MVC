<head>
    <meta charset="utf-8" />
    <title>Force | @(ViewData["Title"]??"管理后台")</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/apps/css/google-fonts.css" rel="stylesheet" type="text/css" />
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/font-awesome/css/font-awesome.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" />
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/simple-line-icons/simple-line-icons.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" />
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/bootstrap/css/bootstrap.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" />
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/uniform/css/uniform.default.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" />
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/bootstrap-daterangepicker/daterangepicker.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" 
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN THEME GLOBAL STYLES -->
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/global/css/components.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" id="style_components" type="text/css" />
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/global/css/plugins.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" />
    <!-- END THEME GLOBAL STYLES -->
    <!-- BEGIN THEME LAYOUT STYLES -->
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/layouts/layout/css/layout.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" />
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/layouts/layout/css/themes/darkblue.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" id="style_color" />
    <link href="@ConfigurationManager.Resource["ResourceApi"]/assets/layouts/layout/css/custom.min.css?v=@ConfigurationManager.Resource["Vsersion"]" rel="stylesheet" type="text/css" />
    <!-- END THEME LAYOUT STYLES -->
    <link rel="shortcut icon" href="~/favicon.ico" />
</head>
<!-- END HEAD -->

<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
    <!-- BEGIN HEADER -->
    @await Component.InvokeAsync("Head")
    <!-- END HEADER -->
    <!-- BEGIN HEADER & CONTENT DIVIDER -->
    <div class="clearfix"> </div>
    <!-- END HEADER & CONTENT DIVIDER -->
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <!-- BEGIN SIDEBAR -->
        <div class="page-sidebar-wrapper">
            <!-- BEGIN SIDEBAR -->
            <!-- DOC: Set data-auto-scroll="false" to disable the sidebar from auto scrolling/focusing -->
            <!-- DOC: Change data-auto-speed="200" to adjust the sub menu slide up/down speed -->
            <div class="page-sidebar navbar-collapse collapse">
                <!-- BEGIN SIDEBAR MENU -->
                @await Component.InvokeAsync("Menu")
                <!-- END SIDEBAR MENU -->
                <!-- END SIDEBAR MENU -->
            </div>
            <!-- END SIDEBAR -->
        </div>
        <!-- END SIDEBAR -->
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
            <!-- BEGIN CONTENT BODY -->
            <div class="page-content">
                <!-- BEGIN PAGE HEADER-->
                <!-- BEGIN THEME PANEL -->
                @await Html.PartialAsync("_Theme")
                <!-- END THEME PANEL -->
                <!-- BEGIN PAGE BAR -->
                @await Html.PartialAsync("_SubHeader")
                <!-- END PAGE BAR -->
                <!-- BEGIN PAGE TITLE-->
                @await Html.PartialAsync("_PageTitle")
                <!-- END PAGE TITLE-->
                <!-- END PAGE HEADER-->
                <!-- BEGIN DASHBOARD-->
                @RenderBody()
                <!-- END DASHBOARD -->
            </div>
            <!-- END CONTENT BODY -->
        </div>
        <!-- END CONTENT -->
    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    @await Html.PartialAsync("_Footer")
    <!-- END FOOTER -->
    <!--[if lt IE 9]>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/respond.min.js?v=@ConfigurationManager.Resource["Vsersion"]"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/excanvas.min.js?v=@ConfigurationManager.Resource["Vsersion"]"></script>
    <![endif]-->
    <!-- BEGIN CORE PLUGINS -->
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/jquery.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/bootstrap/js/bootstrap.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/js.cookie.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
