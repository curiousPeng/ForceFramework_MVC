    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/jquery.blockui.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/uniform/jquery.uniform.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/moment.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/bootstrap-daterangepicker/daterangepicker.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/counterup/jquery.waypoints.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/counterup/jquery.counterup.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/plugins/bootstrap-growl/jquery.bootstrap-growl.min.js" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/global/scripts/app.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <!-- END THEME GLOBAL SCRIPTS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <!-- END PAGE LEVEL SCRIPTS -->
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/layouts/layout/scripts/layout.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/layouts/layout/scripts/demo.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <script src="@ConfigurationManager.Resource["ResourceApi"]/assets/layouts/global/scripts/quick-sidebar.min.js?v=@ConfigurationManager.Resource["Vsersion"]" type="text/javascript"></script>
    <!-- END THEME LAYOUT SCRIPTS -->
